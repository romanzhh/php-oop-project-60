{
  "name": "hexlet/code",
  "bin": [
    "bin/code"
  ],
  "type": "library",
  "description": "hexlet php-package",
  "scripts": {
    "test": "phpunit tests"
  },
  "require": {
    "illuminate/collections": "8.22",
    "nesbot/carbon": "3.0",
    "symfony/string": "7.0",
    "phpstan/phpstan": "1.10"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "phpstan/phpstan-phpunit": "1.3",
    "phpstan/extension-installer": "1.3",
    "symfony/var-dumper": "7.0",
    "phpunit/phpunit": "11.0",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Hexlet\\Validator\\": "src/Validator"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hexlet\\Validator\\Tests\\": "tests"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "romanzh",
      "email": "storkanderon222@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
